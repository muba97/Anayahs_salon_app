    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "graphql-tools": "^6.0.10",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
